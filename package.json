{
  "name": "proyectocuartagen2",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:api": "npm --workspace @proyectocuartagen2/apiveterinaria run dev",
    "dev:web": "npm --workspace @proyectocuartagen2/webveterinaria run dev -- --port 3002",
    "dev:mobile": "npm --workspace @proyectocuartagen2/mobileveterinaria exec npx expo start",
    "dev:mobile-limpia": "npm --workspace @proyectocuartagen2/mobileveterinaria exec npx expo start -c",
    "dev:all": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "prod:api": "npm --workspace @proyectocuartagen2/apiveterinaria start"
  },
  "dependencies": {
    "mysql2": "^3.16.3",
    "socks": "^2.8.7"
  }
}
