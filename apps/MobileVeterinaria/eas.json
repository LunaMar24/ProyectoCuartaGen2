{
  "cli": {
    "version": ">= 18.0.5",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_USE_COMMUNITY_AUTOLINKING": "1"
      },
      "cache": {
        "disabled": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_USE_COMMUNITY_AUTOLINKING": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
